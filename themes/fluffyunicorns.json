{
    "author": "Verena Vertonghen",
    "name": "fluffyunicorns",
    "type": "dark",
    "tokenColors": [
        {
          "name": "Global settings",
          "settings": {
            "background": "#22313F",
            "foreground": "#FFFFFF"
          }
        },
        {
          "name": "String",
          "scope": "string",
          "settings": {
            "foreground": "#FF6AA4"
          }
        },
        {
          "name": "Boolean",
          "scope": "constant.language.boolean",
          "settings": {
            "foreground": "#D6A4FB"
          }
        },
        {
          "name": "Number",
          "scope": "constant.numeric",
          "settings": {
            "foreground": "#FFCA5E"
          }
        },
        {
          "name": "Identifier",
          "scope": "variable, support.variable, support.class, support.constant, meta.definition.variable entity.name.function",
          "settings": {
            "foreground": "#fff6ff"
          }
        },
        {
          "name": "Keyword",
          "scope": "keyword, modifier, variable.language.this, support.type.object, constant.language",
          "settings": {
            "foreground": "#D6A4FB"
          }
        },
        {
          "name": "Function call",
          "scope": "entity.name.function, support.function",
          "settings": {
            "foreground": "#FFCA5E"
          }
        },
        {
          "name": "Storage",
          "scope": "storage.type, storage.modifier",
          "settings": {
            "foreground": "#D6A4FB"
          }
        },
        {
          "name": "Modules",
          "scope": "support.module, support.node",
          "settings": {
            "foreground": "#fff6ff",
            "fontStyle": "italic"
          }
        },
        {
          "name": "Type",
          "scope": "support.type",
          "settings": {
            "foreground": "#7FFFE0"
          }
        },
        {
          "name": "Type",
          "scope": "entity.name.type, entity.other.inherited-class",
          "settings": {
            "foreground": "#7FFFE0"
          }
        },
        {
          "name": "Comment",
          "scope": "comment",
          "settings": {
            "foreground": "#6693bd",
            "fontStyle": "italic"
          }
        },
        {
          "name": "Class",
          "scope": "entity.name.type.class",
          "settings": {
            "foreground": "#7FFFE0",
            "fontStyle": "underline"
          }
        },
        {
          "name": "Class variable",
          "scope": "variable.object.property",
          "settings": {
            "foreground": "#7FFFE0"
          }
        },
        {
          "name": "Class method",
          "scope": "meta.definition.method entity.name.function",
          "settings": {
            "foreground": "#7FFFE0"
          }
        },
        {
          "name": "Function definition",
          "scope": "meta.function entity.name.function",
          "settings": {
            "foreground": "#7FFFE0"
          }
        },
        {
          "name": "Template expression",
          "scope": "template.expression.begin, template.expression.end",
          "settings": {
            "foreground": "#D6A4FB"
          }
        },
        {
          "name": "YAML key",
          "scope": "entity.name.tag.yaml",
          "settings": {
            "foreground": "#fff6ff"
          }
        },
        {
          "name": "JSON key",
          "scope": "meta.object-literal.key, meta.object-literal.key string, support.type.property-name.json",
          "settings": {
            "foreground": "#fff6ff"
          }
        },
        {
          "name": "JSON constant",
          "scope": "constant.language.json",
          "settings": {
            "foreground": "#D6A4FB"
          }
        },
        {
          "name": "CSS class",
          "scope": "entity.other.attribute-name.class",
          "settings": {
            "foreground": "#D6A4FB"
          }
        },
        {
          "name": "CSS ID",
          "scope": "entity.other.attribute-name.id",
          "settings": {
            "foreground": "#FF6AA4"
          }
        },
        {
          "name": "CSS tag",
          "scope": "source.css entity.name.tag",
          "settings": {
            "foreground": "#7FFFE0"
          }
        },
        {
          "name": "HTML tag outer",
          "scope": "meta.tag, punctuation.definition.tag",
          "settings": {
            "foreground": "#D6A4FB"
          }
        },
        {
          "name": "HTML tag inner",
          "scope": "entity.name.tag",
          "settings": {
            "foreground": "#fff6ff"
          }
        },
        {
          "name": "HTML tag attribute",
          "scope": "entity.other.attribute-name",
          "settings": {
            "foreground": "#FFCA5E"
          }
        },
        {
          "name": "Markdown heading",
          "scope": "markup.heading",
          "settings": {
            "foreground": "#D6A4FB"
          }
        },
        {
          "name": "Markdown link text",
          "scope": "text.html.markdown meta.link.inline, meta.link.reference",
          "settings": {
            "foreground": "#fff6ff"
          }
        },
        {
          "name": "Markdown list item",
          "scope": "text.html.markdown beginning.punctuation.definition.list",
          "settings": {
            "foreground": "#D6A4FB"
          }
        },
        {
          "name": "Markdown italic",
          "scope": "markup.italic",
          "settings": {
            "foreground": "#fff6ff",
            "fontStyle": "italic"
          }
        },
        {
          "name": "Markdown bold",
          "scope": "markup.bold",
          "settings": {
            "foreground": "#fff6ff",
            "fontStyle": "bold"
          }
        },
        {
          "name": "Markdown bold italic",
          "scope": "markup.bold markup.italic, markup.italic markup.bold",
          "settings": {
            "foreground": "#fff6ff",
            "fontStyle": "italic bold"
          }
        },
        {
          "name": "INI property name",
          "scope": "keyword.other.definition.ini",
          "settings": {
            "foreground": "#fff6ff"
          }
        },
        {
          "name": "INI section title",
          "scope": "entity.name.section.group-title.ini",
          "settings": {
            "foreground": "#D6A4FB"
          }
        },
        {
          "name": "C# class",
          "scope": "source.cs meta.class.identifier storage.type",
          "settings": {
            "foreground": "#7FFFE0",
            "fontStyle": "underline"
          }
        },
        {
          "name": "C# class method",
          "scope": "source.cs meta.method.identifier entity.name.function",
          "settings": {
            "foreground": "#7FFFE0"
          }
        },
        {
          "name": "C# function call",
          "scope": "source.cs meta.method-call meta.method, source.cs entity.name.function",
          "settings": {
            "foreground": "#FFCA5E"
          }
        },
        {
          "name": "C# type",
          "scope": "source.cs storage.type",
          "settings": {
            "foreground": "#7FFFE0"
          }
        },
        {
          "name": "C# return type",
          "scope": "source.cs meta.method.return-type",
          "settings": {
            "foreground": "#7FFFE0"
          }
        },
        {
          "name": "C# preprocessor",
          "scope": "source.cs meta.preprocessor",
          "settings": {
            "foreground": "#6693bd"
          }
        },
        {
          "name": "C# namespace",
          "scope": "source.cs entity.name.type.namespace",
          "settings": {
            "foreground": "#FFFFFF"
          }
        },
        {
          "name": "Global settings",
          "settings": {
            "background": "#22313F",
            "foreground": "#FFFFFF"
          }
        }
      ],
      "colors": {
        "focusBorder": "#D6A4FB",
        "foreground": "#FFFFFF",
        "dropdown.background": "#364e64",
        "input.background": "#364e64",
        "inputOption.activeBorder": "#D6A4FB",
        "list.activeSelectionBackground": "#D6A4FB80",
        "list.dropBackground": "#D6A4FB80",
        "list.focusBackground": "#D6A4FB80",
        "list.hoverBackground": "#FFFFFF1a",
        "list.inactiveSelectionBackground": "#FFFFFF33",
        "activityBar.background": "#2f4458",
        "activityBar.dropBackground": "#D6A4FB80",
        "activityBarBadge.background": "#D6A4FB",
        "sideBar.background": "#283a4b",
        "sideBarSectionHeader.background": "#2f4458",
        "editorGroup.dropBackground": "#D6A4FB80",
        "editorGroupHeader.tabsBackground": "#283a4b",
        "tab.border": "#00000033",
        "tab.inactiveBackground": "#2f4458",
        "editor.background": "#22313F",
        "editor.foreground": "#FFFFFF",
        "editorLineNumber.foreground": "#FFFFFF4d",
        "editor.lineHighlightBorder": "#FFFFFF1a",
        "editor.rangeHighlightBackground": "#FFFFFF0d",
        "editorWidget.background": "#283a4b",
        "editorHoverWidget.background": "#283a4b",
        "editorMarkerNavigation.background": "#283a4b",
        "peekView.border": "#D6A4FB",
        "peekViewEditor.background": "#1b2732",
        "peekViewResult.background": "#283a4b",
        "peekViewTitle.background": "#22313F",
        "panel.background": "#283a4b",
        "panel.border": "#FFFFFF1a",
        "panelTitle.activeBorder": "#FFFFFF80",
        "panelTitle.inactiveForeground": "#FFFFFF80",
        "statusBar.background": "#1b2732",
        "statusBar.debuggingBackground": "#D6A4FB",
        "statusBar.noFolderBackground": "#1b2732",
        "statusBarItem.activeBackground": "#D6A4FB80",
        "statusBarItem.hoverBackground": "#FFFFFF1a",
        "titleBar.activeBackground": "#1b2732",
        "pickerGroup.border": "#FFFFFF1a",
        "debugToolBar.background": "#2f4458"
      }
}